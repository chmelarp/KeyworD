CREATE TABLE colourdemo.languages
(
  "locale" character varying(32) NOT NULL,
  language character varying(32) NOT NULL,
  family character varying(256),
  letters "char"[],
  percentages real[],
  stopwords character varying(32)[],
  CONSTRAINT languages_pkey PRIMARY KEY (language )
);

